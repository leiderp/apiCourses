{
  "name": "api_cursos",
  "version": "1.0.0",
  "description": "api cursos - prueba tecnica",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon -L ./bin/www",
    "db:create": "cd ./src/database && npx sequelize-cli db:create",
    "db:seed:all": "cd ./src/database && npx sequelize-cli db:seed:all",
    "db:seed:undo": "cd ./src/database && npx sequelize-cli db:seed:undo",
    "db:seed:generate": "cd ./src/database && npx sequelize-cli seed:generate",
    "db:migrate": "cd ./src/database && npx sequelize-cli db:migrate",
    "db:migrate:undo": "cd ./src/database && npx sequelize-cli db:migrate:undo",
    "db:migration:generate": "cd ./src/database && npx sequelize-cli migration:generate",
    "db:model:generate": "cd ./src/database && npx sequelize-cli model:generate"
  },
  "author": "leiderp",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "mysql2": "^3.9.2",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
